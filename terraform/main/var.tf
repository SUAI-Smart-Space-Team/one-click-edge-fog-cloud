variable "ovirt_url" {
    description = "oVirt API URL"
    default = "https://DOMAIN_NAME/ovirt-engine/api"
}
variable "ovirt_username" {
    description = "oVirt Admin user"
    default     = "USERNAME"
}
variable "ovirt_password" {
    description = "oVirt Admin password"
    default     = "PASSWD"
}

